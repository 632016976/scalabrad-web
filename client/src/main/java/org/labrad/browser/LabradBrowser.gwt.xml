<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="labradbrowser">
  <!-- Inherit the core Web Toolkit stuff -->
  <inherits name="com.google.gwt.user.User"/>

  <!-- Other module inherits -->
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.json.JSON"/>
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.sksamuel.gwt.GwtWebsockets"/>
  <inherits name="elemental.Elemental"/>
  <inherits name="org.fusesource.restygwt.RestyGWT"/>
  <inherits name="org.labrad.browser.common.LabradBrowserCommon"/>

  <!-- Configure logging -->
  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>

  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true"/>

  <!-- Compile for chrome only (for testing) -->
  <set-property name="user.agent" value="safari" />

  <!-- Specify the app entry point class -->
  <entry-point class="org.labrad.browser.client.LabradBrowser"/>
</module>
